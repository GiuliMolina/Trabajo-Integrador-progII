<%- include('./partials/head') %>
<body>
	<%- include('./partials/header') %>

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12 container_register">
				<form action="/users/register" method="post">
					<p> User:<input type="text" name="user" placeholder="user name" value=""> </p>
					<p> Email:<input type="email" name="email" placeholder="email" value=""> </p>
					<p> Password:<input type="password" name="password" placeholder="password" value=""> </p>
					<p> Date of birth:<input type="date" name="fecha_de_nacimiento" placeholder="fecha de nacimiento" value=""> </p>
					<p> DNI:<input type="number" name="dni" placeholder="DNI" value=""> </p>
					<!-- <p> Foto de perfil: <input type="text" name="foto de perfil" placeholder="foto de perfil"> </p> -->
					<button type="submit" class="registrarse">Registrarse<i class="fas fa-search"></i></button>
				</form>
			</h2>
		</div>
	</div>

	
     <%- include('./partials/footer')%>
</body>

</html>